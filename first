subjects =["Team","Age"]
dataset= df.groupby("Team")[subjects].mean()
print(dataset)
plt.figure(figsize = (60,20))
plt.bar(x = df["Team"],height =  df["Age"])
plt.xticks(rotation=45)
plt.xlabel("TEAM NAME")
plt.ylabel("MAX AGE")
plt.show()
